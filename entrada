let
	float j, i, n;
	integer myFistInteiro;

	function float media(float zi, bool b, integer x) 
	 let
	   float z, v, hg, g;
	 in
	  z:= 2;
      return z;
	end;
in
	j:=0.5;

	if (myFistInteiro = 2) then
	    myFistInteiro:= myFistInteiro + 1;
	fi;

	n:=3+j;

	read myFistInteiro;
